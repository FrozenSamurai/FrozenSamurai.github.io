from xml.etree.ElementInclude import include


s= "aa"
p = ".*f"
match = True
for q in range(len(s)):
    if len(p)<q+1:
        if (s[q] == p[q]) & match:
            continue
    elif (".*" in p) & match:
        continue
    elif( p[q] == "*") & match:
        break
    else:
        match = False
        break
print(match)